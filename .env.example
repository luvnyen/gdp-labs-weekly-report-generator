# GitHub Service Configuration
# Required for pulling PR data, commits, and reviews

GITHUB_PERSONAL_ACCESS_TOKEN=

GITHUB_USERNAME=

REPO_OWNER=GDP-ADMIN

# Comma-separated repository names
# Example: CATAPA-API,CATAPA-APPS-STATE,CATAPA-APPS-CALENDAR
REPOS=

# ────────────────────────────────────────────────────────────────────────────────

# SonarQube Service Configuration
# Required for code coverage metrics

SONARQUBE_USER_TOKEN=

# Comma-separated project:path pairs
# Examples:
# - Single: catapa-core:src/main/java/com/catapa/core/personnel
# - URL-encoded: catapa-accounts-fe%3Asrc%2Fapp%2Fcore
# - Multiple: catapa-core:src/main/java/com/catapa/core/personnel,catapa-accounts-fe%3Asrc%2Fapp%2Fcore,catapa-web:src/app/modules
SONARQUBE_COMPONENTS=

# ────────────────────────────────────────────────────────────────────────────────

# Google Services Configuration
# Required for Calendar, Forms, and Gmail integration

# Path to Google OAuth client secret JSON file
# Example: client_secret_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com.json
GOOGLE_CLIENT_SECRET_FILE=

# ────────────────────────────────────────────────────────────────────────────────

# Gmail Service Configuration
# Required for email draft creation

# Comma-separated recipient emails
# Example: john.doe@gdplabs.id,jane.smith@gdplabs.id,team.lead@gdplabs.id
GMAIL_SEND_TO=

# Comma-separated CC recipient emails
# Example: manager@gdplabs.id,project.owner@gdplabs.id
GMAIL_SEND_CC=

# ────────────────────────────────────────────────────────────────────────────────

# LLM Service Configuration
# Required for PR summarization

GOOGLE_GEMINI_API_KEY=

GROQ_API_KEY=
